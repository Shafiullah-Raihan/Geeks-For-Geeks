//{ Driver Code Starts
#include <bits/stdc++.h>
using namespace std;


// } Driver Code Ends
ðŸ˜Ž
class Solution {
  public:
    // Function to return Breadth First Traversal of given graph.
    vector<int> bfsOfGraph(vector<vector<int>> &adj) {
       int n = adj.size();
       vector<bool>vis(n,false);
       queue<int>q;
       vector<int>v;
       q.push(0);
       vis[0]= true;
       
       while(!q.empty())
       {
           int frnt = q.front();
           v.push_back(frnt);
           vis[frnt] = true;
           q.pop();
           for(auto u:adj[frnt])
           {
               if(!vis[u] ){
                   q.push(u);
                   vis[u] = true;
               }
           }
       }
       return v;
    }
};
ðŸ˜Ž
//{ Driver Code Starts.

int main() {
    int tc;
    cin >> tc;
    while (tc--) {
        int V, E;
        cin >> V >> E;

        // Now using vector of vectors instead of array of vectors
        vector<vector<int>> adj(V);

        for (int i = 0; i < E; i++) {
            int u, v;
            cin >> u >> v;
            adj[u].push_back(v);
            adj[v].push_back(u); // For undirected graph, add both u->v and v->u
        }

        Solution obj;
        vector<int> ans = obj.bfsOfGraph(adj);
        for (int i = 0; i < ans.size(); i++) {
            cout << ans[i] << " ";
        }
        cout << endl;
    }
    return 0;
}

// } Driver Code Ends
