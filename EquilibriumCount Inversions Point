#include <bits/stdc++.h>
using namespace std;


// } Driver Code Ends
ðŸ˜Ž
class Solution {
  public:
    
    
    int my_counter = 0;

  
    void merge(vector<int> &arr, int p, int q, int r) {
        int l = q - p + 1;
        int l2 = r - q;

        vector<int> arr1(l), arr2(l2);

        
        for (int i = 0; i < l; i++) {
            arr1[i] = arr[i + p];
        }
      
        for (int i = 0; i < l2; i++) {
            arr2[i] = arr[q + i + 1];
        }

        int left = 0, right = 0, k = p;

        
        while (left < l && right < l2) {
            if (arr1[left] <= arr2[right]) {
                arr[k++] = arr1[left++];
            } else {
                arr[k++] = arr2[right++];
                
                my_counter += (l - left);
            }
        }

       
        while (left < l) {
            arr[k++] = arr1[left++];
        }

      
        while (right < l2) {
            arr[k++] = arr2[right++];
        }
    }

    
    void mergeSort(vector<int> &arr, int p, int r) {
        if (p < r) {
            int q = (p + r) / 2;

            mergeSort(arr, p, q);
            mergeSort(arr, q + 1, r);

            
            merge(arr, p, q, r);
        }
    }
    
    int inversionCount(vector<int> &arr) {
        my_counter = 0; 
        mergeSort(arr, 0, arr.size() - 1);
        return my_counter;
    }
};
