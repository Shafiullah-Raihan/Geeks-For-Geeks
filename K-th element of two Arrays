//{ Driver Code Starts
#include <bits/stdc++.h>
using namespace std;


// } Driver Code Ends

ðŸ˜Ž
class Solution {
  public:
    int kthElement(vector<int>& a, vector<int>& b, int k) {
         int n = a.size(), m = b.size();

  
    vector<int> arr(n + m);
    int i = 0, j = 0, d = 0;
    while (i < n && j < m) {

       
        if (a[i] < b[j])
            arr[d++] = a[i++];
        
       
        else
            arr[d++] = b[j++];
    }

    while (i < n)
        arr[d++] = a[i++];

    while (j < m)
        arr[d++] = b[j++];

    return arr[k - 1];
    }
};
ðŸ˜Ž
//{ Driver Code Starts.

// Driver code
int main() {
    int t;
    cin >> t;
    cin.ignore();
    while (t--) {
        int n, m, k;
        cin >> k;
        cin.ignore();
        string input;
        int num;
        vector<int> a, b;

        getline(cin, input);
        stringstream s2(input);
        while (s2 >> num) {
            a.push_back(num);
        }

        getline(cin, input);
        stringstream s3(input);
        while (s3 >> num) {
            b.push_back(num);
        }

        Solution ob;
        cout << ob.kthElement(a, b, k) << endl << "~\n";
    }
    return 0;
}
// } Driver Code Ends
