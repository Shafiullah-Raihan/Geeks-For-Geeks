// User function template for C++

class Solution {
  public:
    int kthMissing(vector<int> &arr, int k) {
       unordered_set<int>st (arr.begin(), arr.end());
       int cnt=0,start=0;
       while(cnt<k){
           start++;
           if(st.find(start) == st.end())
           {
               cnt++;
           }
       }
       return start;
    }
};
ðŸ˜Ž
//{ Driver Code Starts.

int main() {
    int test_case;
    cin >> test_case;
    cin.ignore();
    while (test_case--) {

        int d;
        vector<int> arr, brr, crr;
        string input;
        getline(cin, input);
        stringstream ss(input);
        int number;
        while (ss >> number) {
            arr.push_back(number);
        }
        getline(cin, input);
        ss.clear();
        ss.str(input);
        while (ss >> number) {
            crr.push_back(number);
        }
        d = crr[0];
        int n = arr.size();
        Solution ob;
        int ans = ob.kthMissing(arr, d);
        cout << ans << endl;

        cout << "~"
             << "\n";
    }
    return 0;
}
