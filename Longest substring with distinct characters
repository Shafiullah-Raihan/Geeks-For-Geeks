//{ Driver Code Starts
// Initial Template for C++
#include <bits/stdc++.h>
using namespace std;


// } Driver Code Ends
ðŸ˜Ž
class Solution {
  public:
    int longestSubstrDistinctChars(string& s) {
       
    unordered_set<char> charSet; 
    int res = 0;                 
    int i = 0, j = 0;           
    int n = s.length();          

    while (j < n) {
        if (charSet.find(s[j]) == charSet.end()) { 
            charSet.insert(s[j]);
            res = max(res, j - i + 1); 
            j++; 
        } else {
            charSet.erase(s[i]);
            i++; 
        }
    }
    return res; 

    }
};
ðŸ˜Ž

//{ Driver Code Starts.

int main() {

    int t;
    cin >> t;
    while (t--) {

        Solution obj;
        string s;
        cin >> s;
        cout << obj.longestSubstrDistinctChars(s) << endl;
        cout << "~"
             << "\n";
    }

    return 0;
}

