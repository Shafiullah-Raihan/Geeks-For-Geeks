// User function template for C++

class Solution {
  public:
    int majorityElement(vector<int>& arr) {
        unordered_map<int,int>mp;
        
        for(auto u:arr){
            mp[u]++;
        }
        for(auto u:mp){
            if(u.second>arr.size()/2){
                return u.first;
            }
        }
        return -1;
    }
};
ðŸ˜Ž
//{ Driver Code Starts.

int main() {

    int t;
    cin >> t;
    cin.ignore();
    while (t--) {
        int n;
        vector<int> a, b;
        string input;
        getline(cin, input);
        stringstream ss(input);
        int num;
        while (ss >> num)
            a.push_back(num);

        Solution obj;
        cout << obj.majorityElement(a) << endl;
        cout << "~\n";
    }

    return 0;
}

