//{ Driver Code Starts
#include <bits/stdc++.h>
using namespace std;

ðŸ˜Ž
// } Driver Code Ends
// User function Template for C++
class Solution {
  public:
    bool canAttend(vector<vector<int>> &arr) {
        
        sort(arr.begin(),arr.end());
        int preS =0,preE = 0;
        for(auto u:arr){
            if(preS > u[0] || preE > u[0]) {
                return false;
            }
            preS = u[0], preE = u[1];
        }
        return true;
        
    }
};

ðŸ˜Ž
//{ Driver Code Starts.
int main() {
    int tc;
    cin >> tc;
    while (tc--) {
        int n;
        cin >> n;
        vector<vector<int>> arr(n);
        for (int i = 0; i < n; i++) {
            int x, y;
            cin >> x >> y;
            arr[i].push_back(x);
            arr[i].push_back(y);
        }
        cin.ignore();
        Solution obj;
        bool ans = obj.canAttend(arr);
        if (ans)
            cout << "true" << endl;
        else
            cout << "false" << endl;
    }
    return 0;
}
// } Driver Code Ends
