#include <bits/stdc++.h>
using namespace std;


// } Driver Code Ends
ðŸ˜Ž
class Solution {
  public:
    bool isParenthesisBalanced(string& s) {
       
        vector<char> stack; 
        for (auto& ch : s) {
            if (ch == '(' || ch == '{' || ch == '[') {
                stack.push_back(ch); 
            } else {
                if (!stack.empty() && 
                    ((stack.back() == '(' && ch == ')') || 
                     (stack.back() == '{' && ch == '}') || 
                     (stack.back() == '[' && ch == ']'))) {
                    stack.pop_back(); 
                } else {
                    return false; 
                }
            }
        }
        return stack.empty();
    }
};
ðŸ˜Ž
//{ Driver Code Starts.

int main() {
    int t;
    string a;
    cin >> t;
    while (t--) {
        cin >> a;
        Solution obj;
        if (obj.isParenthesisBalanced(a))
            cout << "true" << endl;
        else
            cout << "false" << endl;

        cout << "~"
             << "\n";
    }
}
