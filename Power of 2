// User function Template for C++
ðŸ˜Ž
class Solution {
  public:
    bool isPowerofTwo(int n) {
         if (n <= 0) {
            return false;
        }
        return (n & (n - 1)) == 0;
    }
};
ðŸ˜Ž
//{ Driver Code Starts.

// Driver code
int main() {

    int t;
    cin >> t; // testcases

    for (int i = 0; i < t; i++) {
        int n; // input a number n
        cin >> n;
        Solution ob;
        if (ob.isPowerofTwo(n))
            cout << "true" << endl;
        else
            cout << "false" << endl;
        cout << "~" << endl;
    }

    return 0;
}
// } Driver Code Ends
