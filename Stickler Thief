// } Driver Code Ends
ðŸ˜Ž
class Solution {
  public:
    // Function to find the maximum money the thief can get.
    int findMaxSum(vector<int>& arr) {
        // Your code here
        int n= arr.size();
        vector<int>dp(n,0);
        dp[0] = arr[0], dp[1] = max(arr[0],arr[1]);
        for(int i=2;i<n;i++)
        {
            dp[i] = max(arr[i]+dp[i-2],dp[i-1]);
        }
        return dp[n-1];
        
    }
};

ðŸ˜Ž
//{ Driver Code Starts.

int main() {

    // taking total testcases
    int t;
    cin >> t;
    cin.ignore();
    while (t--) {
        vector<int> arr;
        string input;
        getline(cin, input);
        stringstream ss(input);
        int number;
        while (ss >> number) {
            arr.push_back(number);
        }

        Solution ob;
        // calling function findMaxSum()
        cout << ob.findMaxSum(arr) << endl;
        cout << "~" << endl;
    }
    return 0;
}

