#include <bits/stdc++.h>
using namespace std;

// } Driver Code Ends
// User function Template for C++
ðŸ˜Ž
class Solution {
  public:
    int isDigitSumPalindrome(int n) {
        int sum =0;
        while(n>0){
            sum += n%10;
            n = n/10;
        }
        string s = to_string(sum);
        bool ok = true;
        int len = s.size();
        for(int i =0;i<len/2;i++){
            if(s[i] != s[len-1 -i]){
                ok = false;
                break;
            }
        }
        return  (ok)?1:0;
    }
};
ðŸ˜Ž
//{ Driver Code Starts.
int main() {
    int t;
    cin >> t;
    while (t--) {
        int N;
        cin >> N;
        Solution ob;
        cout << ob.isDigitSumPalindrome(N) << "\n";
    
cout << "~" << "\n";
}
}

